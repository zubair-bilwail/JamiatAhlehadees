@model CommonLayer.CommonModels.AddHalqa
@{
    ViewBag.Title = "Details";
}

<div class="box-header with-border">
    <h3 class="box-title">Halqa Details</h3>
</div>
<br />

<div class="col-lg-6"></div>
<table class="table table-hover table-responsive">
<tbody>
 
    <tr><th>HalqaName</th><td>@Model.HalqaName</td></tr>
    <tr><th>Area</th><td>@Model.Area</td></tr>
    <tr><th>CreateDate</th><td>@Model.CreatedDate</td></tr>
    <tr><th>CreateBy</th><td>@Model.CreatedBy</td></tr>
    <tr><th>Status</th><td>@Model.Status</td></tr>
</tbody>
</table>
<button value="Edit"
        class="btn btn-primary btn-sm" type="button" id="addressSearch"
        onclick="location.href='@Url.Action("Create", "AddHalqa" ,new {id = Model.Id })'">
    Edit
</button> | 
<button value="Back"
        class="btn btn-info btn-sm" type="button" id="addressSearch"
        onclick="location.href='@Url.Action("Index")'">
    Back
</button> |
<button value="Delete"
        class="btn btn-danger btn-sm" type="button" id="addressSearch"
        onclick="location.href='@Url.Action("Delete", "AddHalqa" ,new {id = Model.Id })'">
    Delete
</button>
